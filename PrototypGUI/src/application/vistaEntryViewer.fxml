<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet vista.css?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane fx:id="vistaProfile" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <VBox alignment="CENTER">
      <children>
            <Pane prefHeight="400.0" prefWidth="630.0">
               <children>
                  <TableView fx:id="table" editable="true" prefHeight="392.0" prefWidth="630.0">
                     <columns>
                        <TableColumn fx:id="attributeColumn" prefWidth="48.0" text="Atrybut" />
                        <TableColumn fx:id="valueColumn" prefWidth="51.0" text="Wartość" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </Pane>
        <Button fx:id="button" alignment="CENTER" mnemonicParsing="false" onAction="#buttonAction" text="Edytuj" />
      </children>
    </VBox>
  </children>
</StackPane>
